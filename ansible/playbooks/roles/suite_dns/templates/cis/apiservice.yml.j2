apiVersion: apiregistration.k8s.io/v1
kind: APIService
metadata:
  name: "v1alpha1.{{ groupName }}"
  namespace: "{{ certManager.namespace }}"
  labels:
    app: "cert-manager-webhook-ibm-cis"
  annotations:
    cert-manager.io/inject-ca-from: "{{ certManager.namespace }}/cert-manager-webhook-ibm-cis-serving-cert"
spec:
  group: "{{ groupName }}"
  groupPriorityMinimum: 1000
  versionPriority: 15
  service:
    name: cert-manager-webhook-ibm-cis
    namespace: "{{ certManager.namespace }}"
  version: v1alpha1
